#include <iostream>
#include <time.h>
#include <stdlib.h>
#include <set>
#define RANGE 100
#define random() ((rand())%RANGE)
using namespace std;
int main() {
    srand((unsigned)time(NULL));
    int n;
    cin>>n;
    /*生成n个随机数，可有重复*/
    for(int i=0;i<n;i++) cout<<random()<<" ";
    cout<<endl;
    /*生成n个随机数，互不重复*/
    set<int> v;
    while(v.size()!=n) v.insert(random());
    for(set<int>::iterator it=v.begin();it!=v.end();it++)
        cout<<*it<<" ";
    return 0;
}
